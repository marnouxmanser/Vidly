@model Vidly.Models.Customer

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul style="list-style-type:circle;">
    <li>@Model.MembershipType.Description</li>
    @if (Model.BirthDate != null)
    {
        <li>@Model.BirthDate</li>
    }
</ul>
